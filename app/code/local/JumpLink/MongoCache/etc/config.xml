<?xml version="1.0"?>
<config>
  <modules>
    <JumpLink_MongoCache>
      <version>0.0.1</version>
    </JumpLink_MongoCache>
  </modules>
  <global>
    <helpers>
      <jumplink_mongocache>
        <class>JumpLink_MongoCache_Helper</class>
      </jumplink_mongocache>
    </helpers>
  </global>
  <frontend>
    <routers>
      <jumplink_mongocache>
        <use>standard</use>
        <args>
          <module>JumpLink_MongoCache</module>
          <frontName>mongocache</frontName>
        </args>
      </jumplink_mongocache>
    </routers>
  </frontend>
  <adminhtml>
    <acl>
      <resources>
        <all>
          <title>Allow Everything</title>
        </all>
        <admin>
          <children>
            <system>
              <children>
                <config>
                  <children>
                    <mongocache>
                      <title>Mediabox - All</title>
                    </mongocache>
                  </children>
                </config>
              </children>
            </system>
          </children>
        </admin>
      </resources>
    </acl>
  </adminhtml>
</config>